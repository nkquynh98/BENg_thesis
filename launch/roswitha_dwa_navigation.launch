<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">

    <rosparam file="$(find indoor_thesis)/Config_TEB_ROSWITHA/costmap_common_params_2d.yaml" command="load" ns="global_costmap" /> 
    <rosparam file="$(find indoor_thesis)/Config_TEB_ROSWITHA/costmap_common_params_2d.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find indoor_thesis)/Config_TEB_ROSWITHA/local_costmap_params.yaml" command="load" />

    <rosparam file="$(find indoor_thesis)/Config_TEB_ROSWITHA/global_costmap_params.yaml" command="load" /> 
    <rosparam file="$(find indoor_thesis)/Config_TEB_ROSWITHA/dwa_local_planner_params.yaml" command="load" />
    <param name="base_local_planner" value="dwa_local_planner/DWAPlannerROS"/>
    <param name="controller_frequency" value="10.0" />
    <param name="controller_patience" value="15.0" />

   </node>

</launch> 
